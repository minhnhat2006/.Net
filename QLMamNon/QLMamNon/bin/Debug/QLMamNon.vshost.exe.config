<?xml version="1.0"?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="QLMamNon.Properties.Settings.qlmamnonConnectionString1"
      connectionString="server=localhost;User Id=root;Password=root;Persist Security Info=True;database=qlmamnon;Character Set=utf8"
      providerName="MySql.Data.MySqlClient" />
  </connectionStrings>
  <startup>
